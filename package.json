{
  "name": "stun",
  "version": "1.1.0",
  "description": "Session Traversal Utilities for NAT (STUN) server.",
  "main": "index.js",
  "scripts": {
    "test": "xo && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodertc/stun.git"
  },
  "keywords": [
    "webrtc",
    "stun",
    "rfc5389",
    "5389",
    "ice",
    "turn"
  ],
  "author": "Dmitry Tsvettsikh <me@reklatsmasters.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodertc/stun/issues"
  },
  "homepage": "https://github.com/nodertc/stun#readme",
  "devDependencies": {
    "jest": "^21.0.0",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "binary-data": "^0.1.0",
    "buffer-xor": "^2.0.1",
    "crc": "^3.4.4",
    "ipaddr.js": "^1.4.0",
    "is-stun": "^2.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "ignores": [
      "**/test/**"
    ]
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": "test/(.*)\\.js$",
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "/test/"
    ]
  }
}
